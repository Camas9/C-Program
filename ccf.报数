#include<stdio.h>
int main(){
    int n,i;
 int a=0,b=0,c=0,d=0;
 scanf("%d",&n);
 for(i=1;i<=n;i++){
  if((i%7==0||i%10==7||i/100==7||i/10%10==7)&&(i%4==1)){
   a++;
   n++;
  }
  if((i%7==0||i%10==7||i/100==7||i/10%10==7)&&(i%4==2)){
   b++;
   n++;
  }
  if((i%7==0||i%10==7||i/100==7||i/10%10==7)&&(i%4==3)){
   c++;
   n++;
  }
  if((i%7==0||i%10==7||i/100==7||i/10%10==7)&&(i%4==0)){
   d++;
   n++;
  }
 }
 printf("%d\n%d\n%d\n%d\n",a,b,c,d);
 return 0;
}
