#include<stdio.h>
int main()
{
    char a[3][3];
    int i,j;
    for(i=0;i<3;++i)
    {
        for(j=0;j<3;++j)
          scanf("%c ", &a[i][j]);
    }
    if(a[0][0]=='K'&&a[1][1]=='K'&&a[2][2]=='K'||
       a[0][0]=='K'&&a[1][0]=='K'&&a[2][0]=='K'||
       a[0][1]=='K'&&a[1][1]=='K'&&a[2][1]=='K'||
       a[0][2]=='K'&&a[1][2]=='K'&&a[2][2]=='K'||
       a[0][0]=='K'&&a[0][1]=='K'&&a[0][2]=='K'||
       a[1][0]=='K'&&a[1][1]=='K'&&a[1][2]=='K'||
       a[2][0]=='K'&&a[2][1]=='K'&&a[2][2]=='K'||
       a[0][2]=='K'&&a[1][1]=='K'&&a[2][0]=='K')
       printf("KiKi wins!");
    else if
      (a[0][0]=='B'&&a[1][1]=='B'&&a[2][2]=='B'||
       a[0][0]=='B'&&a[1][0]=='B'&&a[2][0]=='B'||
       a[0][1]=='B'&&a[1][1]=='B'&&a[2][1]=='B'||
       a[0][2]=='B'&&a[1][2]=='B'&&a[2][2]=='B'||
       a[0][0]=='B'&&a[0][1]=='B'&&a[0][2]=='B'||
       a[1][0]=='B'&&a[1][1]=='B'&&a[1][2]=='B'||
       a[2][0]=='B'&&a[2][1]=='B'&&a[2][2]=='B'||
       a[0][2]=='B'&&a[1][1]=='B'&&a[2][0]=='B')
       printf("BoBo wins!");
    else printf("No winner!");
    return 0;
}
